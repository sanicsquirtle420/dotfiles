#SAME FONT AS USED IN ~/.config/i3/config

[mediaplayer]
label=ﱘ
command=playerctl metadata --format '{{artist}} - {{title}}'
instance=spotify,%any,vlc
interval=5
signal=10
color=#A7C7E7

[weather]
label=摒
command=curl -Ss 'https://wttr.in?0&T&Q' | cut -c 16- | head -2 | xargs echo
interval=3600
color=#C3B1E1

[date]
label=
command=date +"%a. %b. %d"
interval=5
color=#77dd77

[time]
label=
command=date +"%H:%M:%S"
interval=1
color=#FAC898
